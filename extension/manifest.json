{
  "name": "InstaLogin",
  "description": "Instantly become a verified user on any site",
  "version": "1.1",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "/16x16.png",
      "32": "/32x32.png",
      "48": "/48x48.png",
      "128": "/128x128.png"
    }
  },
  "host_permissions": [
    "https://instalogin.co/*"
  ],
  "permissions": ["activeTab", "scripting", "storage", "alarms", "notifications"]
}
